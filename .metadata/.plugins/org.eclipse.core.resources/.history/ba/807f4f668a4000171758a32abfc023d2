	<script>
										
		$(document).ready(function() {
			$("#control").show();
			$("#jogos").hide();
			$("#equipes").hide();

			$("#menu_jogos").on("click", function(e) {
				$("#control").hide();
				$("#jogos").show();
				$("#equipes").hide();
			});
			$("#menu_control").on("click", function(e) {
				$("#control").show();
				$("#jogos").hide();
				$("#equipes").hide();
			});
			
			$("#treeview_opcoes").on("click", "a", function(e) {
				$("#control").hide();
				$("#jogos").hide();
				$("#equipes").show();	
			});
			
	/*		$("#menu_equipes_0").on("click", function(e) {
				$("#control").hide();
				$("#jogos").hide();
				$("#equipes").show();
			});
			$("#menu_equipes_1").on("click", function(e) {
				$("#control").hide();
				$("#jogos").hide();
				$("#equipes").show();
			});
			$("#menu_equipes_2").on("click", function(e) {
				$("#control").hide();
				$("#jogos").hide();
				$("#equipes").show();
			});
			$("#menu_equipes_3").on("click", function(e) {
				$("#control").hide();
				$("#jogos").hide();
				$("#equipes").show();
			});
		*/	
			$("#add_team").on("click", function(e) {
				$("#control").show();
				$("#jogos").hide();
				$("#equipes").hide();
			});

			$("#btn_sumula").on("click", function(e){
				$("#modal_change_game").hide();
				$("modal_sumula").show();
				$("modal_sumula_1").hide();
				
			});
			
			$("#btn_sumula_1").on("click", function(e){
				$("#modal_change_game").hide();
				$("modal_sumula").hide();
				$("modal_sumula_1").show();

			});		
	
			$("#btn_salve_change").on("click", function(e){
				
			});		
	
			
			//PARA MARCAR O VERMELHO AUTOMATICO
			$('#sumula input').on('change', function() {
   				//alert($('input[name=optradio]:checked', '#sumula').val());
   				if ($('input:checked', '#sumula').val() == 2){
   					$(":checkbox", $(this).parents("tr")).prop("checked", true);
   				}
   				
   				//if ($('#chk').prop("checked", true)){
   					//if($('input[name=optradio]:checked', '#sumula').val() == 1){
   						//$("#chk").prop("checked",false);	
   					//}
   				//}
			});
			
			//PARA ADICIONAR TIME NA TABELA LATERAL 
			
			$("#btn_add_team").click(function(){
				 var branches = $('<li"><a href="#" id="menu_equipes_' +
					$("#treeview_opcoes").children().length + '">' 
					+' <i class="fa fa-circle-o"></i>'+ $("#name_new_team").val()+
					'</a></li>').prependTo("#treeview_opcoes");
				 teste();
			});
			
			
		});
	</script>


	<script>
		function teste(){			
			$('[id^="menu_equipes"]').on("click", function(e) {
				$("#control").hide();
				$("#jogos").hide();
				$("#equipes").show();
			});
		}

	</script>


